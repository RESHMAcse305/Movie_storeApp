@model Movie_StoreApp.Models.Movie
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>ReleaseDate</th>
            <th>Actors</th>
            <th>Duration</th>
            <th>Rating</th>
            <th>Genre_Name</th>
            <th>Director_Name</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Movie_Id</td>
            <td>@Model.Movie_Title</td>
            <td>@Model.ReleaseDate</td>
            <td>@Model.Actors</td>
            <td>@Model.Duration</td>
            <td>@Model.Rating</td>
            <td>@Model.Genre_Name</td>
            <td>@Model.Director_Name</td>
            <td>
                <a asp-action="Review_load"  asp-route-movieId="@Model.Movie_Id" class="btn btn-primary">
                    Add Review
                </a>
            </td>
        </tr>
    </tbody>

</table>
